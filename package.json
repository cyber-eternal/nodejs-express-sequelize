{
  "name": "boilerplate-nodejs-express-sequelize",
  "version": "1.0.0",
  "description": "Boilerplate based on nodejs, express, sequelize, docker",
  "author": "Edgar Sargsyan",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && NODE_ENV=dev && babel ./src --out-dir ./dist/ --ignore ./node_modules/ --copy-files",
    "start": "node ./dist/index",
    "start:dev": "clear && NODE_ENV=dev nodemon src/index.js --exec babel-node | bunyan",
    "start:prod": "clear && NODE_ENV=prod && ENVIRONMENT=prod nodemon src/index.js --exec babel-node | bunyan",
    "test:lint": "eslint .",
    "clean": "rm -rf ./dist"
  },
  "keywords": [
    "Nodejs",
    "babel",
    "express",
    "boilerplate",
    "Template",
    "mysql",
    "docker"
  ],
  "license": "MIT",
  "dependencies": {
    "apidoc": "^0.53.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "1.20.1",
    "bunyan": "1.8.12",
    "cors": "2.8.5",
    "dotenv": "^8.2.0",
    "express": "4.21.2",
    "mysql2": "^2.1.0",
    "sequelize": "^6.29.0",
    "sequelize-cli": "^5.5.1",
    "sequelize-paginate": "^1.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-private-methods": "^7.8.3",
    "@babel/plugin-syntax-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-stage-3": "^7.8.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "1.0.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "core-js": "^3.0.0-beta.11",
    "eslint": "^5.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "nodemon": "2.0.20"
  },
  "nodemonConfig": {
    "ignore": [
      "./doc",
      "../doc"
    ]
  }
}
